<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Pizza Final</display-name>
  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>signup</servlet-name>
    <servlet-class>Signup</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>contactus</servlet-name>
    <servlet-class>ContactUs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>contactus</servlet-name>
    <url-pattern>/contact</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>shopcatalog</servlet-name>
  	<servlet-class>ShopCatalog</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>shopcatalog</servlet-name>
	<url-pattern>/shopcatalog</url-pattern>
	
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>newaddress</servlet-name>
  	<servlet-class>NewDeliveryAddress</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>newaddress</servlet-name>
	<url-pattern>/newaddress</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>subscribe</servlet-name>
  	<servlet-class>Subscribe</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>subscribe</servlet-name>
  	<url-pattern>/subscribe</url-pattern>
  </servlet-mapping>
  
  
</web-app>